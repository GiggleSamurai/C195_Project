<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="150.0" layoutY="125.0" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
         <children>
            <Label text="Username:" />
            <TextField fx:id="UsernameTextField" />
            <Label text="Password" />
            <TextField fx:id="PasswordTextField" />
            <Button mnemonicParsing="false" onAction="#LoginButton" text="Login" />
         </children>
      </VBox>
   </children>
</AnchorPane>
